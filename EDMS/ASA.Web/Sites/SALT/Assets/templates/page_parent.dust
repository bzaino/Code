        <meta name="apple-mobile-web-app-capable" content="yes"/>
        <meta http-equiv="cleartype" content="on"/>

        {>"WebtrendsTags" /}

        <title>
            {+dynamicTitle}
                Salt: Education Unlocked. Dreams Unlimited.
            {/dynamicTitle}
        </title>

        <!-- SWD-4786 removed some og tags in favor or dynamic script -->
        {#content}
            {#mainContent}
            <meta property="og:description" content="{mainContent[0].record.attributes.page_description}" />
            <meta property="og:title" content="{mainContent[0].record.attributes.resource_link_title[0]}"/>
            {/mainContent}
        {/content}
        <meta property="fb:app_id" content="398864066837152" />
        <meta property="og:site_name" content="SALT" />
        <meta property="og:type" content="website" />
        <meta property="og:locale" content="en_us" />
        <meta property="og:locale" content="en_US"/>
        <meta name="twitter:card" content="summary"/>
        <meta name="twitter:site" content="@SALT_MONEY"/>
        <meta name="twitter:domain" content="SALT Blog"/>
        <meta name="twitter:creator" content="@SALT_MONEY"/>
        <meta name="p:domain_verify" content="4d8700bc7be7740602f7d547c79bd5a2"/>

        {+dynamicHead}
        {/dynamicHead}

        <!-- using separate files for now. IE9 stops loading css stylesheets that are more than around 200kb -->
        <link href="/assets/css/normalize.css" rel='stylesheet' />
        <link href="/assets/css/foundation5/foundation.css" rel='stylesheet' />
        <link href="/assets/css/f5/concat.css" rel='stylesheet'/>

        {+dynamicCSS}
        {/dynamicCSS}
        <!--[if lt IE 9]>
        <script src="/assets/scripts/js/libs/selectivizr.js"> </script>
        <script src="/assets/scripts/js/libs/css3-mediaqueries.js"> </script>
        <![endif]-->


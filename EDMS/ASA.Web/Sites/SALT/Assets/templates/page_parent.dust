        <!-- custom modernizr needed for the HTML5 fallback shim -->
        <script src="/Assets/Scripts/OptimizedJS/js/libs/modernizr/modernizr.js"></script>
        <script data-main="/assets/scripts/OptimizedJS/js/" src="/assets/scripts/OptimizedJS/js/require.js"></script>
        <script src="/assets/scripts/OptimizedJS/js/require-config.js"></script>
        <script src="/assets/scripts/OptimizedJS/js/main.js"></script>

        <script>
            /* please, do not remove SaltDustHelpers from the require statement below */
            require(['jquery', 'modules/SaltDustHelpers'], function ($) {
                /* stop all "a" tags with no href from refreshing the page */
                /* needs to be here in page_parent to gurantee that the listener will be attached before any other listeners can */
                $(document.body).on('click', '.js-void-href', function (e) {
                    e.preventDefault();
                });
            });
            /*include this polyfill file for responsive images*/
            if (!window.HTMLPictureElement) {
                require(['libs/picturefill.min']);
            }
        </script>

        {+dynamicScripts}
        {/dynamicScripts}
        <link rel="shortcut icon" href="https://www.saltmoney.org/favicon.ico" />
        <link rel="apple-touch-icon" href="https://www.saltmoney.org/apple-touch-icon.png" />
    </head>
    <body class="main_article">
        <div class="off-canvas-wrap" data-offcanvas>
            <div class="inner-wrap">
                <!--[if lt IE 7]>
                    <p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p>
                <![endif]-->
                <!-- Salt Logo for Printing (only visible in print preview) -->
                <div class="for-print-logo">
                    <img src="/Assets/images/chat/salt-logo.gif">
                </div>
                <a class="js-rr-skip-link skip-link js-void-href" tabindex="1" href>
                    <span>Skip to Right Column</span>
                </a>
                <a class="js-f-skip-link skip-link js-void-href" tabindex="1" href>
                    <span>Skip to Footer</span>
                </a>
                <!--Main Header-->
                {+dynamicTopBar}
                        {>"salt_header" /}
                {/dynamicTopBar}
                <!--End Main Header-->
                    {! dynamic Hero allows you to add a carousel or banner image partial if needed !}
                    {+dynamicHero}
                    {/dynamicHero}
                    <!-- Articles Content -->
                    {! This is the page blocker for the registration wall on unauth !}
                    {^SiteMember.ActiveDirectoryKey}
                        <a href="" class="js-void-href registrationOverlay-link" data-window-shade="registrationOverlay" tabindex="-1"><div class="blocker" class="hide"> </div></a>
                        <!-- SWD-7216 Route 'small screen' users to signup page instead of signup overlay -->
                        <a href="/register" class="registration-link" ><div class="blocker" class="hide" tabindex="-1"> </div></a>
                    {/SiteMember.ActiveDirectoryKey}
                    <section id="content" class="content-section {?individualIDCookie}{?SiteMember.DashboardEnabled}salt3 {/SiteMember.DashboardEnabled}{/individualIDCookie}">
                        {>"ContentBlock" /}
                    </section>

                    <!-- End Articles Content -->

                    <!--Site Intecept-->
                    {?ProdIntercept}
                        {>"Modules/SiteIntercept" /}
                    {:else}
                        {>"Modules/NonProdSiteIntercept" /}
                    {/ProdIntercept}

                    <!--Main Footer-->
                    <div class="js-footer">
                        {>"foundation5_salt_footer" /}
                    </div>
                    <!--End Main Footer-->
                    <a href="" class="exit-off-canvas"></a>

                {+singlePageApp}
                {/singlePageApp}

                <script>
                    /* initialization of foundation, and HTML5 fallback shims */
                    require(['jquery', 'libs/fastclick', 'jquery.dotdotdot', 'foundation5', 'libs/polyfiller/polyfiller'], function ($, fastClick) {
                        $.webshims.setOptions({
                          waitReady: false
                        });
                        $.webshims.polyfill(['forms', 'forms-ext']);
                        $(document).ready(function () {
                            $('.tileText').dotdotdot({
                                wrap: 'word',
                                watch: 'window',
                                ellipsis: '... ',
                                height: null
                            });
                            /* SWD-5800 */
                            fastClick.attach(document.body);
                            $(document).foundation({
                                abide: {
                                    live_validate: false,
                                    focus_on_invalid: true,
                                    error_labels: false,
                                    patterns: {
                                        alpha: /^(?!\s)([a-zA-Z'\-\s]{1,30})$/,
                                        alpha_numeric: /^[a-zA-Z0-9]{1,30}$/,
                                        integer: /-?\d+/,
                                        number: /^[0-9]*$/,
                                        interest: /(^[0-9]{1,2})+(\.[0-9]{1,2})?$/,
                                        balance: /(^[0-9]{1,9})+(\.[0-9]{1,2})?$/,
                                        password: /^\S{8,32}$/,
                                        reporting_id: /^(?!\s)([a-zA-Z0-9-_@. ]{1,100})$/,
                                        email: /^(?!^.{65})((''[\w-\s]+'')|([\w-]+(?:\.[\w-]+)*)|(''[\w-\s]+'')([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-zA-Z]{2,6}(?:\.[a-zA-Z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/,
                                        zip_code: /^\d{5}$/,
                                        profile_other: /^.{1,20}$/
                                    }
                                },
                                tooltips: {
                                    selector : '.has-tip',
                                    additional_inheritable_classes : [],
                                    tooltip_class : '.tooltip',
                                    touch_close_text: 'tap to close',
                                    disable_for_touch: true,
                                    tip_template : function (selector, content) {
                                        return '<span data-selector="' + selector + '" class="' + Foundation.libs.tooltips.settings.tooltipClass.substring(1) + '">' + content + '<span class="nub"></span></span>';
                                    }
                                }
                            });
                        });

                        $('head').append(
                            '<meta property="og:url" content="'+window.location.href+'"/>' +
                            '<meta property="og:image:secure_url" content="https://www.saltmoney.org/Assets/images/logos/salt-logo_v2.gif"/>' +
                            '<meta property="og:image:type" content="image/gif"/>' +
                            '<meta property="og:image:width" content="137"/>' +
                            '<meta property="og:image:height" content="75"/>'
                        );
                    });
                </script>

                <div id="scroll-waypoint"></div>
                {+dynamicFooter}
                {/dynamicFooter}

                <!-- SWD-3864 Reporting ID -->
                {>"Overlays/Enrollment"/}
            </div>
        </div>
    </body>
</html>

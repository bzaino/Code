<extensions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation="extensions.xsd">
    <extension id="xmgr"
            defaultName="Experience Manager"
            defaultDescription="Manage the customer experience by creating and modifying dynamic pages and content items."
            role="content"
            url="/ifcr/sites/${EAC_APP}/content.html?timestamp=${TS}&amp;auth=${AUTH}"
            launchImageUrl="/images/infront_flyingpanels.png"
            helpUrl="cwbh_working_root.html"
            sharedSecret="DLK#*@#%Gu3897hr*#FI$fil#H2oHP@"
            height="800"/>
    <extension id="previewsettings"
            defaultName="Preview Settings"
            defaultDescription="Configure the Preview application."
            role="settings"
            url="/ifcr/sites/${EAC_APP}/configuration/tools/preview.html?timestamp=${TS}&amp;auth=${AUTH}"
            helpUrl="cwbh_settings_root.html"
            sharedSecret="DLK#*@#%Gu3897hr*#FI$fil#H2oHP@"
            height="800"/>
    <extension id="usersegmentmanager"
            defaultName="User Segments"
            defaultDescription="Create and manage user segments to tailor the content that is displayed to site visitors based on their identity."
            url="/ifcr/sites/${EAC_APP}/userSegments.html?timestamp=${TS}&amp;auth=${AUTH}"
            role="settings"
            launchImageUrl="/images/infront_person.png"
            helpUrl="cwbh_profile_root.html"
            sharedSecret="DLK#*@#%Gu3897hr*#FI$fil#H2oHP@"
            height="800"/>
    <extension id="thesaurusmanager"
            defaultName="Thesaurus"
            defaultDescription="Create and manage entries in the text search thesaurus."
            role="thesaurus"
            url="/ifcr/sites/${EAC_APP}/thesaurus.html?timestamp=${TS}&amp;auth=${AUTH}"
            launchImageUrl="/images/infront_T.png"
            helpUrl="cwbh_thesaurus_root.html"
            sharedSecret="DLK#*@#%Gu3897hr*#FI$fil#H2oHP@"
            height="800"/>
    <extension id="redirectsmanager"
            defaultName="Keyword Redirects"
            defaultDescription="Automatically redirect users to another page whenever they search for a specific search term."
            role="redirects"
            url="/ifcr/sites/${EAC_APP}/redirects.html?timestamp=${TS}&amp;auth=${AUTH}"
            launchImageUrl="/images/infront_redirects.png"
            sharedSecret="DLK#*@#%Gu3897hr*#FI$fil#H2oHP@"
            height="800"/>
	<extension id="promotecontent"
            defaultName="Promote Exp. Mgr. Changes"
            defaultDescription="Promote Experience Manager Content to Production"

<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>@db.databaseName@</TargetDatabaseName>
    <DeployScriptFileName>@db.databaseName@.sql</DeployScriptFileName>
	<TargetConnectionString>Server=@db.serverName@;User Id=@db.username@;Password=@db.password@;</TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>
	<ScriptDatabaseOptions>False</ScriptDatabaseOptions>
    <BlockOnPossibleDataLoss>@db.blockOnPossibleDataLoss@</BlockOnPossibleDataLoss>
	<IgnoreFilegroupPlacement>False</IgnoreFilegroupPlacement>
	<DoNotAlterReplicatedObjects>False</DoNotAlterReplicatedObjects>
  </PropertyGroup>
</Project>